{
    "name": "Skip YouTube Advertisement",
    "version": "1.0",
    "description": "Chrome Extension to skip YT ADs",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["skipytad.js"]
    }],
    "icons": {
        "16": "yt_skip_ads16.png",
        "32": "yt_skip_ads32.png",
        "48": "yt_skip_ads48.png",
        "128": "yt_skip_ads128.png"
    }
}